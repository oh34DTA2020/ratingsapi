{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "name": "getratings",
      "databaseName": "customer-ratings",
      "collectionName": "ratings",
      "connectionStringSetting": "CosmosDBConnection",
      "direction": "in",
      "Id": "{Query.ratingId}",
      "PartitionKey": "{Query.partitionKeyValue}"
    }
  ]
}
